{
    "version": 2,
    "builds": [
        {
            "src": "back/api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "front/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "back/api/index.js" }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}